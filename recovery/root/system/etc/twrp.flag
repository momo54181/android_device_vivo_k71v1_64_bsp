# Android fstab file.
# <mount_point>           <fstype>  <device>        <device2>                           <flags>
/oem                      ext4     /dev/block/platform/bootdevice/by-name/cust          flags=display=oem
/system                   ext4     /dev/block/platform/bootdevice/by-name/system        flags=display="System";wipeingui;
/system_image             emmc     /dev/block/platform/bootdevice/by-name/system        flags=display="System image";flashimg=1;
/vendor                   ext4     /dev/block/platform/bootdevice/by-name/vendor        flags=display=Vendor";wipeingui;
/vendor_image             emmc     /dev/block/platform/bootdevice/by-name/vendor        flags=display="Vendor image";flashimg=1;
/data                     ext4     /dev/block/platform/bootdevice/by-name/userdata      flags=encryptable=/dev/block/platform/bootdevice/by-name/metadata
/cache                    ext4     /dev/block/platform/bootdevice/by-name/cache         flags=display=cache
/mnt/vendor/nvdata        ext4     /dev/block/platform/bootdevice/by-name/nvdata        flags=display=nvdata
/mnt/vendor/nvcfg         ext4     /dev/block/platform/bootdevice/by-name/nvcfg         flags=display=nvcfg
/mnt/vendor/persist       ext4     /dev/block/platform/bootdevice/by-name/persist       flags=display=persist
/persist_image            emmc     /dev/block/platform/bootdevice/by-name/persist       flags=display="Persist image";backup=1;flashimg=1;
/persistent               emmc     /dev/block/platform/bootdevice/by-name/frp           flags=display=persistent
/nvram                    emmc     /dev/block/platform/bootdevice/by-name/nvram         flags=display=nvram
/bootloader               emmc     /dev/block/platform/bootdevice/by-name/lk            flags=display=bootloader
/bootloader2              emmc     /dev/block/platform/bootdevice/by-name/lk2           flags=display=bootloader2
/misc                     emmc     /dev/block/platform/bootdevice/by-name/para          flags=display=misc
/recovery                 emmc     /dev/block/platform/bootdevice/by-name/recovery      flags=display=recovery
/boot                     emmc     /dev/block/platform/bootdevice/by-name/boot          flags=display=boot
/logo                     emmc     /dev/block/platform/bootdevice/by-name/logo          flags=display=logo
/metadata                 emmc     /dev/block/platform/bootdevice/by-name/metadata
/odmdtbo                  emmc     /dev/block/platform/bootdevice/by-name/odmdtbo       flags=display=odmdtbo
/dtbo                     emmc     /dev/block/platform/bootdevice/by-name/dtbo          flags=display=dtbo
/external_sd              vfat     /dev/block/mmcblk1p1    /dev/block/mmcblk1           flags=display="Micro SDCard";storage;wipeingui;removable
